{
    "name": "micseres/micro-client-bundle",
    "description": "Micro Client Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "version": "1.0.2",
    "authors": [
        {
            "name": "zogxray",
            "email": "zogxray@gmail.com"
        },
        {
            "name": "Andrii Yakovlev",
            "email": "yawa20@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-sockets": "*",
        "ext-json": "*",
        "symfony/dependency-injection": ">=4.0|^5.0",
        "symfony/http-kernel": "^4.1|^5.0",
        "micseres/micro-client-reactor": "dev-master",
        "symfony/config": "^4.1|^5.0"

    },
        "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Micseres\\MicroClientBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Micseres\\MicroClientBundle\\Tests\\": "Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
